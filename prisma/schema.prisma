// prisma/schema.prisma
datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

generator client {
  provider = "prisma-client-js"
}

model User {
  id       Int     @id @default(autoincrement())
  username String  @unique
  role     String
  password String
}

model GoodsInLog {
  id        Int      @id @default(autoincrement())
  timestamp DateTime @default(now())
  barcode   String
  name      String
  sku       String
  quantity  Int
  location  String
  shelf     String
}
